<h1>{{device.name}}'s Sensors</h1>
  <p>This page lists all the sensors of {{device.name}}</p>

  <table class='table table-striped table-bordered'>
    <thead>
      <tr>
        <th>Sensors</th>
     </tr>
    </thead>
    <tbody>
	{{#each device.sensors}}
        <tr>
       <td>{{this}}
       </td>
       </tr>
    {{/each}}
</tbody>
  </table>